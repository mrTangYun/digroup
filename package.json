{
  "name": "bfftrip-wexin",
  "version": "0.0.1",
  "description": "bfftrip-wexin",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server client tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "dev": "better-npm-run dev",
    "test": "better-npm-run deploy:prod && better-npm-run start:prod",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "build": "better-npm-run deploy:prod",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec node bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "better-npm-run": "^0.0.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^3.8.3",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.8.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.1",
    "react-bootstrap": "^0.30.8",
    "redbox-react": "^1.3.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^1.3.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "antd": "^2.13.12",
    "babel-plugin-import": "^1.2.1",
    "debug": "^2.3.3",
    "express": "^4.16.2",
    "fetch-polyfill": "^0.8.2",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "halogen": "^0.2.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.0",
    "rc-animate": "^2.4.4",
    "rc-queue-anim": "^1.4.1",
    "rc-scroll-anim": "^1.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-transition-group": "^1.1.2",
    "react-videojs": "0.0.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "video.js": "^6.6.3",
    "yargs": "^6.5.0"
  }
}
